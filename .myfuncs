#
#gcc compilation aliases"c89 ,c99 debug mode,rekease mode

alias gd='gcc -ansi -pedantic-errors -Wall -Wextra -g'
alias gc='gcc -ansi -pedantic-errors -Wall -Wextra -DNDEBUG -03'
alias gd9='gcc -std=c99 -pedantic-errors -Wall -Wextra -g'
alias gc9='gcc -std=99 -pedantic-errors -Wall -Wextra -DNDEBUG -03'
#
#Valgrind alias
alas vlg='valgrind --leak-check=key --track-origins=yes'
